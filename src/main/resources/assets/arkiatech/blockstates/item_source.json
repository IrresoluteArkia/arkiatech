{
    "multipart": [
       {   "apply": { "model": "arkiatech:item_pipe_center" }},
       {   "when": { "north": "true" },
           "apply": { "model": "arkiatech:item_pipe_line", "y": 180}
       },
       {   "when": { "east": "true" },
           "apply": { "model": "arkiatech:item_pipe_line", "y": 270}
       },
       {   "when": { "south": "true" },
           "apply": { "model": "arkiatech:item_pipe_line"}
       },
       {   "when": { "west": "true" },
           "apply": { "model": "arkiatech:item_pipe_line", "y": 90}
       },
       {   "when": { "up": "true" },
           "apply": { "model": "arkiatech:item_pipe_line", "x": 90}
       },
       {   "when": { "down": "true" },
           "apply": { "model": "arkiatech:item_pipe_line", "x": 270}
       },
       {   "when": { "northinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source", "y": 180}
       },
       {   "when": { "eastinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source", "y": 270}
       },
       {   "when": { "southinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source"}
       },
       {   "when": { "westinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source", "y": 90}
       },
       {   "when": { "upinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source", "x": 90}
       },
       {   "when": { "downinv": "true" },
           "apply": { "model": "arkiatech:item_pipe_source", "x": 270}
       }
   ]
}
